<?xml version="1.0"?>
<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema" elementFormDefault="qualified">
	<xs:element name="klanten">
		<xs:complexType>
			<xs:sequence>
				<xs:element name="klant" maxOccurs="unbounded" minOccurs="0">
					<xs:complexType>
						<xs:sequence>
							<xs:element name="naam" type="xs:string"/>
							<xs:element name="adres" type="xs:string"/>
							<xs:element name="postcode" type="xs:string"/>
							<xs:element name="plaats" type="xs:string"/>
							<xs:element name="telnr" type="xs:string"/>
							<xs:element name="email" type="xs:string"/>
							<xs:element name="gebDatum" type="xs:date"/>
						</xs:sequence>
						<xs:attribute name="bsn" type="xs:string"/>
					</xs:complexType>
				</xs:element>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
</xs:schema>