<?xml version="1.0"?>
<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema" elementFormDefault="qualified">
	<xs:element name="behandelingen">
		<xs:complexType>
			<xs:sequence>		
				<xs:element name="behandeling">
					<xs:complexType>
						<xs:sequence>
							<xs:element name="status">
								<xs:simpleType>
									<xs:enumeration value="stat1"/>
									<xs:enumeration value="stat2"/>
									<xs:enumeration value="stat3"/>
								</xs:simpleType>
							</xs:element>
							<xs:element name="klant" type="xs:string"/>
							<xs:element name="behandelcode" type="xs:string"/>
							<xs:element name="afspraken">
								<xs:complexType>
									<xs:sequence>
										<xs:element name="afspraak"></xs:element>
									</xs:sequence>
								</xs:complexType>
							</xs:element>
						</xs:sequence>
					</xs:complexType>
				</xs:sequence>
			</xs:complexType>
		</xs:element>
	</xs:element>
</xs:schema>